<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.bsm.mapper.BsmMapper">
    <select id="getTeamList" resultType="com.example.bsm.dto.BsmDto">
        SELECT id
             ,team_name
             ,area
             ,create_date
             ,delete_date
             ,modify_date
        FROM team
    </select>
</mapper>
